{
  "name": "jacks-home-app",
  "version": "0.8.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build --mode=production",
    "lint": "vue-cli-service lint && eslint --fix --ext .js,.vue src && stylelint --fix src/**/*.scss",
    "serve": "vue-cli-service serve"
  },
  "dependencies": {
    "pdfmake": "^0.1.66",
    "typedarray-slice": "1.0.4",
    "uikit": "3.1.5",
    "vue": "^2",
    "es6-object-assign": "1.1.0",
    "vue-loading-overlay": "3.4.3",
    "vue-router": "^3.3.4",
    "vue-select": "^3.4.0",
    "vuelidate": "0.7.7",
    "vuelidate-error-extractor": "2.4.0",
    "vuex": "^3.5.1",
    "vuex-persistedstate": "^3.0.1",
    "register-service-worker": "1.7.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.12.1",
    "eslint-global-patch": "^1.0.4",
    "@vue/cli-plugin-eslint": "3.12.1",
    "@vue/cli-plugin-pwa": "4.5.19",
    "@vue/cli-service": "3.12.1",
    "babel-eslint": "^10",
    "babel-polyfill": "6.26.0",
    "es6-promise": "4.2.8",
    "eslint": "^6",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-standard": "5.0.0",
    "eslint-plugin-vue": "^7",
    "sass": "^1",
    "sass-loader": "^10",
    "stylelint": "10.1.0",
    "stylelint-order": "3.1.1",
    "vue-template-compiler": "^2",
    "webpack": "^4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}